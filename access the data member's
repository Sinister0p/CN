#include <iostream>
#include <string>
using namespace std;

class Student {
public:
    string name;   
    int rollNumber;

    void displayInfo() {
        cout << "Name: " << name << endl;
        cout << "Roll Number: " << rollNumber << endl;
    }
};

int main() {
 
    Student s;

    s.name = "Aanya Singh";
    s.rollNumber = 101;
    s.displayInfo();

    return 0;
}
